{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb/hooks",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react-hooks/exhaustive-deps": "off",
        "react/prop-types": "off",
        "react/prop-types-exhaustive-deps": "off"

    }
}